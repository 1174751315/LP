<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="loadPrediction.domain.SimpleDate" table="SIMPLE_DATE">
        <id name="dateString" column="ID">
            <generator class="assigned"/>
        </id>
        <many-to-one name="dateType" column="DATE_TYPE" class="loadPrediction.domain.SimpleDateType"/>
        <many-to-one name="weatherType" column="WEATHER_TYPE" class="loadPrediction.domain.SimpleDateWeatherType"/>
    </class>
</hibernate-mapping>
