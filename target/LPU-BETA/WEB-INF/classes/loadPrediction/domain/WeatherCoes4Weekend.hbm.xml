<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="loadPrediction.domain.WeatherCoes4Weekend" table="WEATHER_COES_WEEKEND">
        <id name="type" column="TYPE">
            <generator class="assigned"/>
        </id>
        <property name="relationCoe" column="REL_COE"/>
        <property name="maxBase" column="MAX_BASE"/>
        <property name="minBase" column="MIN_BASE"/>
        <property name="weight" column="WEIGHT"/>
    </class>
</hibernate-mapping>
