<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="loadPrediction.core.cache.PredictionCacheEntity" table="PREDICTION_CACHES">
        <id name="dateString" column="DATE_STRING">
            <generator class="assigned"/>
        </id>
        <property name="type" column="TYPE"/>
        <property name="outputImagePath" column="IMG_PATH"/>
        <property name="outputExcelPath" column="XL_PATH"/>
        <property name="outputRptImagePath" column="RPT_IMG_PATH"/>
        <property name="warning" column="WARNING"/>
    </class>
</hibernate-mapping>
