<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="prediction.domain.LoadBase" table="LOAD_PERUNIT_BASE">
        <id name="id" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="year" column="YEAR"/>
        <property name="month" column="MONTH"/>
        <property name="maxLoadBase" column="MAX"/>
        <property name="minLoadBase" column="MIN"/>
        <property name="aveLoadBase" column="AVE"/>
    </class>
</hibernate-mapping>
