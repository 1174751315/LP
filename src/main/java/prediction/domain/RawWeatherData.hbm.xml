<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="prediction.domain.RawWeatherData" table="WEATHER_DATA_RAW">
        <composite-id>
            <key-property name="dateString" column="DATE_STRING"/>
            <key-property name="baseString" column="BASE_DATE_STRING"/>
            <key-property name="cityId" column="CITY_ID"/>
            <key-property name="maxTemperature" column="MAX_TEMP"/>
            <key-property name="minTemperature" column="MIN_TEMP"/>
            <key-property name="aveTemperature" column="AVE_TEMP"/>
            <key-property name="maxHumid" column="MAX_HUMID"/>
            <key-property name="minHumid" column="MIN_HUMID"/>
            <key-property name="aveHumid" column="AVE_HUMID"/>
            <key-property name="maxWindSpeed" column="MAX_WIND_SPEED"/>
            <key-property name="aveWindSpeed" column="AVE_WIND_SPEED"/>
            <key-property name="rainFall" column="RAIN_FALL"/>
        </composite-id>
    </class>
</hibernate-mapping>
