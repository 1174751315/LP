<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="prediction.domain.Accuracy" table="PREDICTION_ACCURACY">
        <id name="dateString" column="PREDICTION_DATE">
            <generator class="assigned"/>
        </id>
        <property name="accuracy" column="ACC"/>
    </class>
</hibernate-mapping>
